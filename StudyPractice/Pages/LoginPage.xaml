<Page x:Class="StudyPractice.Pages.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StudyPractice.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoginPage">

    <Grid>
       
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock Text="Login"></TextBlock>
                <TextBox x:Name="TbLogin" Height="30" Width="100"></TextBox>
                <TextBlock Text="Password"></TextBlock>
                <TextBox x:Name="TbPassword" Height="30" Width="100"></TextBox>
                <Button x:Name="BTest" Content="Test" Click="BTest_Click"  Height="30"></Button>
            </StackPanel>

       
    </Grid>
</Page>
