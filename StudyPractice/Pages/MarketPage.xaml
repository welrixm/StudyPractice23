<Page x:Class="StudyPractice.Pages.MarketPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StudyPractice.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MarketPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Width="200">
            <Button Content="Product" Click="Button_Click"></Button>
            <Button  Content="Order" Click="Button_Click_1"></Button>
            <Button Content="Provider" Click="Button_Click_2"></Button>
            <Button Content="Product type" Click="Button_Click_3"></Button>
            <Button></Button>
            <Button></Button>
            <Button></Button>

        </StackPanel>
        <Frame x:Name="MainFrane" NavigationUIVisibility="Hidden" Grid.Column="1"></Frame>
    </Grid>
</Page>
